{
  "name": "object-templator",
  "version": "0.2.0",
  "description": "Template an object by running sandboxed code",
  "main": "dist/src",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "template",
    "object"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianmandrup/object-templator.git"
  },
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "contributors": [
    "Kristian Mandrup <kmandrup@gmail.com>"
  ],
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "deepmerge": "^2.0.1",
    "json2yaml": "^1.1.0",
    "object-to-xml": "^1.1.0",
    "powerdash": "^1.1.0",
    "run-sandboxed": "^0.1.1",
    "stringify-object": "^3.2.2"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "@types/deepmerge": "^1.3.3",
    "@types/stringify-object": "^3.2.0",
    "@types/jest": "^22.1.4",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2",
    "jest": "^22.4.2"
  }
}
